<definitions>

	<!-- Tsuki Wolf -->
	<npc id="tsuki_wolf" inherit="monster_base">
		<name>a tsuki wolf</name>
		<id>250</id>
		<skin><random valuelist="0x709,0x714,0x719" /></skin>
		<inherit id="KARMA_L4" />
		<inherit id="FAME_L4" />
		<!-- Stats -->
		<str><random min="400" max="450"/></str>
		<dex><random min="150" max="200"/></dex>
		<int><random min="65" max="75"/></int>
		<maxhitpoints><random min="380" max="440" /></maxhitpoints>
		<maxmana>40</maxmana>
		<!-- Damage -->
		<mindamage>47</mindamage>
		<maxdamage>73</maxdamage>
		<tag name="dmg_cold" value="5" type="int" />
		<tag name="dmg_energy" value="5" type="int" />
		<!-- Skills -->
		<skill id="wrestling"><random min="980" max="1080"/></skill>
		<skill id="tactics"><random min="950" max="1100"/></skill>
		<skill id="magicresistance"><random min="650" max="700"/></skill>
		<skill id="anatomy"><random min="650" max="750"/></skill>
		<!-- Resists -->
		<tag name="res_physical" value="40,60" type="int" />
		<tag name="res_fire" value="50,70" type="int" />
		<tag name="res_cold" value="50,70" type="int" />
		<tag name="res_poison" value="50,70" type="int" />
		<tag name="res_energy" value="50,70" type="int" />
		<!-- Loot -->
		<backpack>
			<content>
				<item id="eed"><amount><random min="400" max="500" /></amount></item>
			</content>
		</backpack>
		<lootpacks>bodypart;bones</lootpacks>
		<!-- Carve -->
		<intproperty name="carve_hides" value="25" />
		<intproperty name="carve_meat" value="4" />
		<!-- Misc -->
		<category>Monsters\SE\Tsuki Wolf</category>
		<strproperty name="slayer_group" value="daemons" />
	</npc>

</definitions>
