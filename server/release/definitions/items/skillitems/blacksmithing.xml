	<!-- the following define sections are for application on all types of armor -->
	<define id="iron_armor">
		<name>iron %1</name>
		<color>0x0961</color>
	</define>
	
	<define id="shadow_armor">
		<name>shadow %1</name>
		<color>0x0386</color>
		<defense>1.1</defense> <!-- this means 110 percent of original defense -->
		<durability>1.1</durability>
		<requires type='str'>5</requires> <!-- this means (75) +5 str needed (=80) -->
		<weight>2.0</weight> <!-- 2x weight of base -->
		<value>1.5</value>
	</define>
	
	<define id="merkite_armor">
		<name>merkite %1</name>
		<color>0x02C3</color>
		<defense>1.2</defense>
		<durability>1.2</durability>
		<requires type='str'>10</requires> <!-- this means (75) +10 str needed (=85) -->
		<weight>3.0</weight>
		<value>2.0</value>		
	</define>
	
	<define id="copper_armor">
		<name>copper %1</name>
		<color>0x046E</color>
		<defense>1.3</defense>
		<durability>1.3</durability>
		<requires type='str'>15</requires> <!-- this means (75) +15 str needed (=90) -->
		<weight>4.0</weight>
		<value>2.5</value>		
	</define>

	<define id="silver_armor">
		<name>silver %1</name>
		<color>0x0965</color>
		<defense>1.4</defense>
		<durability>1.4</durability>
		<requires type='str'>20</requires> <!-- this means (75) +15 str needed (=90) -->
		<weight>5.0</weight>
		<value>3.0</value>		
	</define>
	
	<define id="bronze_armor">
		<name>bronze %1</name>
		<color>0x02E7</color>
		<defense>1.5</defense>
		<durability>1.5</durability>
		<requires type='str'>25</requires> 
		<weight>6.0</weight>
		<value>3.5</value>		
	</define>
	
	<define id="golden_armor">
		<name>golden %1</name>
		<color>0x0466</color>
		<defense>1.6</defense>
		<durability>1.6</durability>
		<requires type='str'>30</requires> 
		<weight>7.0</weight>
		<value>4.0</value>		
	</define>
	
	<define id="agapite_armor">
		<name>agapite %1</name>
		<color>0x0150</color>
		<defense>1.7</defense>
		<durability>1.7</durability>
		<requires type='str'>35</requires> 
		<weight>8.0</weight>
		<value>4.5</value>		
	</define>
	
	<define id="verite_armor">
