<definitions>
	<!-- Default Player privs -->
	<acl id="player" rank="1">
		<group name="command">
			<action name="WHERE" permit="true" />
			<action name="TRAVEL" permit="true" />
			<action name="STAT" permit="true" />
			<action name="SKILL" permit="true" />
		</group>
		<group name="multi"></group>
	</acl>

	<!-- Admin privs -->
	<acl id="admin" rank="100">
		<group name="command">
			<action name="any" permit="true" />
		</group>
		<group name="multi">
			<action name="any" permit="true" />
		</group>
		<group name="RemoteAccess">
			<action name="any" permit="true" />
		</group>
	</acl>

	<!-- GameMaster privs -->
	<acl id="gm" rank="50">
		<group name="command">
			<action name="any" permit="true" />
		</group>
		<group name="multi">
			<action name="any" permit="true" />
		</group>
	</acl>

	<!-- Counselor privs -->
	<acl id="counselor" rank="25">
		<group name="command">
			<action name="any" permit="true" />
		</group>
		<group name="multi"></group>
	</acl>
</definitions>
